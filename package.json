{
  "name": "wdio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
"scripts": {
    "test": "npm run preserve-history && npm run clean-results && npm run copy-configs && npm run run-wdio && npm run generate-report && npm run open-report",
    "preserve-history": "if exist allure-report\\history xcopy /E /I /Y allure-report\\history allure-results\\history",
    "clean-results": "rimraf allure-results",
    "copy-configs": "xcopy /E /I /Y allure-configs allure-results",
    "run-wdio": "wdio run ./wdio.conf.js",
    "generate-report": "npx allure generate allure-results --clean",
    "open-report": "npx allure open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.19.2",
    "@wdio/cli": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "allure-commandline": "^2.34.1",
    "rimraf": "^6.0.1"
  }
}
